{
    "port": 81,
    "sslPort": 444,
    "folder": "/path/to/your/music/folder",
    "icon": "icon.png",
    "verbose": true,
    "allow": {
        "audio": ["mp3", "flac", "ogg"],
        "image": ["png", "jpg", "jpeg"],
        "misc": ["txt"]
    },
    "directoryId": "dir",
    "allowedCors": "*",
    "ssl": {
        "key": "/path/to/your/ssl/key.pem",
        "certificate": "/path/to/your/ssl/certificate.crt"
    },
    "protocols": {
        "http": false,
        "https": true
    }
}
